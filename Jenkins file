pipeline{
    agent any
    environment{
        PATH = "/etc/maven/bin:$PATH"
   }
    stages{
        stage("SCM") {
            steps{
              git 'https://github.com/sunildevops77/maven.git'
            }
        }
        stage("Mvn Build") {
            steps{
              sh "mvn clean package"
            }
        }
    }
   
}
